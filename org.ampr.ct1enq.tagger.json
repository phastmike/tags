{
    "app-id" : "org.ampr.ct1enq.tagger",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "45",
    "sdk" : "org.gnome.Sdk",
    "command" : "tagger",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--filesystem=host",
        "--filesystem=home"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "tagger",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/phastmike/tagger.git"
                }
            ]
        }
    ],
    "build-options" : {
        "env" : {        }
    }
}
