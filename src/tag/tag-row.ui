<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="TagsTagRow" parent="AdwActionRow">
    <style>
      <class name="property"/>
      <class name="monospace"/>
    </style>
    <property name="hexpand">true</property>
    <child type="prefix">
      <object class="GtkCheckButton" id="enabled">
        <style>
          <class name="selection-mode"/>
          </style>
      </object>
    </child>
    <child type="suffix">
      <object class="GtkBox">
        <!--
        <style>
          <class name="card"/>
        </style>
        -->
        <child>
          <object class="GtkLabel" id="hitcounter">
            <property name="label">0</property>
            <style>
              <!--<class name="title-4"/>-->
              <class name="numeric"/>
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
